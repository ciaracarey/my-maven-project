<settings>
  <mirrors>
    <mirror>
      <id>cloudsmith-mirror</id>
      <mirrorOf>*</mirrorOf>
      <url>https://dl.cloudsmith.io/${YOUR_ENTITLEMENT_TOKEN}/ciara-demo/acme-nonprod/maven/</url>
    </mirror>
  </mirrors>

  <servers>
    <server>
      <id>cloudsmith-mirror</id>
      <username>${{ secrets.CLOUDSMITH_USERNAME }}</username>
      <password>${{ secrets.CLOUDSMITH_API_KEY }}</password>
    </server>
  </servers>
</settings>
